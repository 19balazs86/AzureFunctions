{
  "version": "2.0",

  "logging": {

    "logLevel": {
      "default": "Warning",
      "Host": "Warning",
      "Function": "Debug"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },

  "extensions": {
    "durableTask": {
      "hubName": "%MyTaskHub%" // The name is configurable in local.settings.json
    }
  }
}